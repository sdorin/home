*loginShell:	true
*scrollBar:	false
*secondaryScroll:	true
*urgentOnBell:	false
URxvt*geometry:         112x22  
URxvt*termName: rxvt-unicode
URxvt.font: -misc-fixed-medium-r-normal--15-140-75-75-c-90-iso8859-1
!URxvt*font:           xft:Monospace:size=9
!	xft:FreeSerif:style=Regular, \
!	xft:Monospace
!URxvt*font:            xft:terminus:size=8
URxvt*urlLauncher:         firefox  
URxvt*scrollBar:        false
URxvt*scrollBar_right:        true
URxvt*scrollColor:        #000000
URxvt*imLocale: en_US.utf8

urxvt*depth: 32
urxvt*background: rgba:1000/1000/1000/dd00
!urxvt*background: [21]#000002

!URxvt.perl-ext-common: ...,tabbed,...
! special
*.foreground:   #b6bbcd
*.background:   #0c0c0c
*.cursorColor:  #b6bbcd

! black
*.color0:       #333333
*.color8:       #505050

! red
*.color1:       #ac4142
*.color9:       #ac4142

! green
*.color2:       #6c813e
*.color10:      #6c813e

! yellow
*.color3:       #b0a96f
*.color11:      #b0a96f

! blue
*.color4:       #586479
*.color12:      #586479

! magenta
*.color5:       #585c79
*.color13:      #585c79

! cyan
*.color6:       #afc4db
*.color14:      #afc4db

! white
*.color7:       #b6bbcd
*.color15:      #b6bbcd
